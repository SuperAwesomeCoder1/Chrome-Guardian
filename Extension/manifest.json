{
  "manifest_version": 2,

  "name": "Chrome Guardian",
  "description": "A extension to prevent online Harassment",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}